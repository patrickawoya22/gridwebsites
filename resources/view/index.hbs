
<!-- set layout -->
{{!< layout}}

<section class="display__cont">
    <div class="grid-container">
        <div class="grid-x grid-padding-x align-center-middle">
            <div class="medium-8 cell">
                <h1>
                    <span class="type-wrap">
                        <span id="typed-strings">
                            <span>Website Design</span>
                            <span>API Integration</span>
                            <span>Email Templates</span>
                            <span>Landing Pages</span>
                        </span>
                        <span id="typed"></span>
                    </span>
                </h1>
                <p class="d__paragraph">
                    Find over 3100 website templates to design landing pages, portfolio sites, blogs, or corporate websites. Explore a curated collection of HTML website templates as well as complementary tools like UI graphics and social media icons to make the web design process much easier.
                </p>
            </div><!-- ./medium cell -->
            <div class="medium-4 cell">

                <ul id="work-content-slider" class="slide__wrapper">
                    <li class="lslide cell-item">
                        <img src="/img/project/encraze-responsive.png" alt="project image">
                    </li><!-- ./medium cell -->
                    <li class="lslide cell-item">
                        <img src="/img/project/leadswork-responsive.png" alt="project image">
                    </li><!-- ./medium cell -->
                    <li class="lslide cell-item">
                        <img src="/img/project/vantworks-responsive.png" alt="project image">
                    </li><!-- ./medium cell -->
                    <li class="lslide cell-item">
                        <img src="/img/project/ultimateresourcesltd-responsive.png" alt="project image">
                    </li><!-- ./medium cell -->
                    <li class="lslide cell-item">
                        <img src="/img/project/ridebliss-responsive.png" alt="project image">
                    </li><!-- ./medium cell -->
                    <li class="lslide cell-item">
                        <img src="/img/project/pooleholidayrentals-responsive.png" alt="project image">
                    </li><!-- ./medium cell -->
                </ul>

            </div>
        </div><!-- ./grid-x grid-padding-x -->
    </div><!-- ./grid-container -->
</section>
<section class="temp__wrapper cont_padding">
    <div class="grid-container">
        <div class="grid-x grid-padding-x">
            <div class="medium-12 cell">
                <h2>Foundation Grid XY Templates</h2>

                    <p>
                        Component-based frameworks designed, built, and supported by foundation.zurb Team.
                    </p>

            </div>

            <div class="medium-12 cell">

                <ul id="foundation-content-slider" class="slide__wrapper">

                    {{#each foundation_templates.hits.hits}}
                        <li class="lslide cell-item">
                            <a href="/product/{{_source.url_name}}/{{_id}}" title="{{_source.name}}">
                                <img src="{{req.customBaseURI}}img/project/img-origin/{{_source.image_obj.hits.hits.0._source.name}}" alt="{{_source.url_name}}">
                            </a>
                                <!--<div>Created by <a href="/product/{{_source.url_name}}/{{_id}}">{{_source.user_obj.hits.hits.0._source.first_name}}-->
                                    <!--{{_source.user_obj.hits.hits.0._source.last_name_short}}-->
                                <!--</a></div>-->
                                <div><span>{{_source.name}}</span><span>${{_source.offer_price}}</span></div>
                                <div>{{_source.main_category}}</div>
                        </li><!-- ./medium cell -->
                    {{/each}}
                </ul>

            </div>

        </div><!-- ./grid-x grid-padding-x -->

        <br>

    </div><!-- ./grid-container -->
</section>

