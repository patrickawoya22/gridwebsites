                <p>Lorem ipsum dolor sit amet, consectetuer adipiscing elit.</p>
            </div>  <!--./medium-8 cell-->

            <div class="medium-12 cell">

                <ul id="email-templates-content-slider" class="slide__wrapper">
                    {{#each email_templates}}
                        <li class="lslide cell-item">
                            <img src="/img/temp/thumb-1/{{main_img}}" alt="{{category_1}}">
                        </li><!-- ./medium cell -->
                    {{/each}}
                </ul>

            </div>
